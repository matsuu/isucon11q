#!/usr/bin/perl

use strict;
use warnings;

use DateTime;

{
    package FakeCal;

    sub utc_rd_values {
        return @{ $_[0] };
    }
}

my $num = shift;

my $days = do { use integer; $num / 86400 };
my $secs = $num % 86400;

my $obj = bless [ $days, $secs, 0 ], 'FakeCal';

my $dt = DateTime->from_object( object => $obj );

print "\n $dt\n\n";
